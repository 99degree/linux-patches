+		break;
+	case CMD_STATUS_RUNNING:
+		snprintf(buffer, sizeof(buffer), "%s", tostring(RUNNING));
+		break;
+	case CMD_STATUS_OK:
+		snprintf(buffer, sizeof(buffer), "%s", tostring(OK));
+		break;
+	case CMD_STATUS_FAIL:
