+	if (enables)
+		ts->touch_functions = (ts->touch_functions | SEC_TS_BIT_SETFUNC_HOVER |
+													 SEC_TS_BIT_SETFUNC_MUTUAL);
+	else
+		ts->touch_functions = ((ts->touch_functions & (~SEC_TS_BIT_SETFUNC_HOVER)) |
+													 SEC_TS_BIT_SETFUNC_MUTUAL);
