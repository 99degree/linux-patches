+static void sec_ts_swap(u8 *a, u8 *b) {
+	u8 temp = *a;
+	*a = *b;
+	*b = temp;
+}
+
+static void rearrange_sft_result(u8 *data, int length) {
+	int i;
+
+	for (i = 0; i < length; i += 4) {
+		sec_ts_swap(&data[i], &data[i + 3]);
+		sec_ts_swap(&data[i + 1], &data[i + 2]);
+	}
+}
